        <div class="flex items-center gap-2">
            <input type="text" id="searchInput" placeholder="역명 검색" class="p-1.5 px-4 rounded text-black text-sm w-80 focus:outline-none shadow-inner" onkeypress="if(event.keyCode==13) handleSearch()">
            <button onclick="handleSearch()" class="bg-[#555588] p-1.5 px-5 rounded text-sm hover:bg-[#444477] font-bold transition-colors">검색</button>
        </div>
    </nav>

    <div class="wiki-container is-transfer shadow-lg">
        <div class="txt-lvl-1">오석역</div>

        <div class="category-box">
            <span class="font-bold text-[#7777AA]">분류:</span> 
            <span class="wiki-link">중구(효빈)의 철도역</span> | <span class="wiki-link">1988년 개업한 철도역</span> | <span class="wiki-link">효빈 도시철도 1호선</span> | <span class="wiki-link">효빈 도시철도 4호선</span>
        </div>

        <div class="wiki-tool-group">
            <a href="talk.html" class="wiki-tool-btn">토론</a>
            <a href="edit.html" class="wiki-tool-btn">편집</a>
            <a href="history.html" class="wiki-tool-btn">역사</a>
            <div class="wiki-tool-btn" onclick="toggleFavorite()">★</div>
        </div>

        <div class="flex flex-col md:flex-row justify-between items-start gap-8 mb-10">
            <div id="toc-box" class="toc shrink-0 shadow-sm h-fit">
                <div class="toc-title text-sm uppercase tracking-widest text-black">목차</div>
                <ul class="space-y-1.5 text-sm font-bold">
                    <li>1. <a href="#s-1" class="wiki-link">개요</a></li>
                    <li>2. <a href="#s-2" class="wiki-link">역 정보</a>
                        <ul class="pl-4">
                            <li>2.1. <a href="#s-2.1" class="wiki-link">역출구 정보</a></li>
                        </ul>
                    </li>
                    <li>3. <a href="#s-3" class="wiki-link">승강장</a>
                        <ul class="pl-4 grid grid-cols-1 gap-1 mt-1">
                            <li>3.1. <a href="#s-3.1" class="wiki-link">1호선</a></li>
                            <li>3.2. <a href="#s-3.2" class="wiki-link">4호선</a></li>
                        </ul>
                    </li>
                    <li>4. <a href="#s-4" class="wiki-link">역 이용객 통계</a></li>
                    <li>5. <a href="#s-5" class="wiki-link">연계 교통</a></li>
                    <li>6. <a href="#s-6" class="wiki-link">기타</a></li>
                </ul>
            </div>

            <aside class="infobox shrink-0 shadow-sm rounded overflow-hidden">
                <div class="infobox-top-station-name"><b>오석</b></div>
                <div class="infobox-line-header">
                    <div class="station-ring line-1">1</div>
                    <div class="station-ring line-4">4</div>
                </div>

                <!-- 인포박스 내비게이션 -->
                <script>
                    const navLines = [
                        {
                            id: '1', name: '1호선', color: 'var(--wiki-line-1)', 
                            prev: '창선', prev_dir: '창선', prev_d: '← 1.09 km', 
                            next: '장원', next_dir: '승남해수욕장', next_d: '0.9 km →',
                            num: '113-1'
                        },
                        {
                            id: '4', name: '4호선', color: 'var(--wiki-line-4)', 
                            prev: '삼각', prev_dir: '해운산업지구', prev_d: '← 0.8 km', 
                            next: '장원', next_dir: '천가', next_d: '1.16 km →',
                            num: '410'
                        }
                    ];
                    navLines.forEach((l) => {
                        document.write(`
                            <div class="infobox-rail-nav">
                                <a href="${l.prev}역.html" class="rail-nav-prev">
                                    ${l.prev_dir} 방면<br>
                                    <b>${l.prev}</b><br>${l.prev_d}
