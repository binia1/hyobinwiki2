    <script>
        function handleSearch() {
            const query = document.getElementById('searchInput').value;
            if (query) {
                showMsg(`"${query}" 검색 기능은 현재 데모 모드입니다.`);
            } else {
                showMsg('검색어를 입력해주세요.');
            }
        }

        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; 
            box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }

        document.addEventListener('DOMContentLoaded', () => {
            const tooltips = document.querySelectorAll('.wiki-fn');
            const tooltipContainer = document.getElementById('wiki-tooltip');

            tooltips.forEach(fn => {
                fn.addEventListener('mouseenter', (e) => {
                    const href = fn.getAttribute('href');
                    if (href && href.startsWith('#')) {
                        const targetId = href.substring(1);
                        const targetEl = document.getElementById(targetId);
                        if (targetEl) {
                            tooltipContainer.innerHTML = targetEl.innerHTML;
                            tooltipContainer.style.display = 'block';
                            
                            const rect = e.target.getBoundingClientRect();
                            const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                            tooltipContainer.style.top = (rect.top + scrollTop - tooltipContainer.offsetHeight - 10) + 'px';
                            tooltipContainer.style.left = (rect.left + 10) + 'px';
                        }
                    }
                });

                fn.addEventListener('mouseleave', () => {
                    tooltipContainer.style.display = 'none';
                });
            });
        });
    </script>

