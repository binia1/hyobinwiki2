    <script>
        function handleSearch() {
            const query = document.getElementById('searchInput').value.trim();
            const contextArea = document.getElementById('wiki-context-area');
            const marks = contextArea.querySelectorAll('mark.highlight');
            marks.forEach(mark => {
                const text = document.createTextNode(mark.textContent);
                mark.parentNode.replaceChild(text, mark);
            });
            contextArea.normalize();

            if (!query) return;

            let foundCount = 0;
            const regex = new RegExp(`(${query.replace(/[.*+?^${}()|[\]\\]/g, '\\$&')})`, 'gi');

            function highlight(node) {
                if (node.nodeType === 3) {
                    const text = node.nodeValue;
                    if (regex.test(text)) {
                        foundCount++;
                        const span = document.createElement('span');
                        span.innerHTML = text.replace(regex, '<mark class="highlight">$1</mark>');
                        node.parentNode.replaceChild(span, node);
                    }
                } else if (node.nodeType === 1 && node.nodeName !== 'SCRIPT' && node.nodeName !== 'STYLE') {
                    Array.from(node.childNodes).forEach(highlight);
                }
            }
            highlight(contextArea);

            const box = document.getElementById('wiki-msg-box');
            box.innerText = foundCount > 0 ? `ì´ ${foundCount}ê±´ì˜ ê²€ìƒ‰ ê²°ê³¼ê°€ ìˆìŠµë‹ˆë‹¤.` : `"${query}" ê²€ìƒ‰ ê²°ê³¼ê°€ ì—†ìŠµë‹ˆë‹¤.`;
            box.style.display = 'block';
            setTimeout(() => box.style.display = 'none', 2500);
        }

        // ëœë¤ ì²«ì°¨ ì‹œê°„ ë°°ì •
        window.onload = function() {
            const min = 4 * 60 + 45; // 04:45
            const max = 5 * 60 + 30; // 05:30
            const randomStart = Math.floor(Math.random() * (max - min + 1)) + min;
            const h = Math.floor(randomStart / 60);
            const m = randomStart % 60;
            const t1 = `0${h}:${m < 10 ? '0'+m : m}`;
            
            // ì—­ë°©í–¥ì€ +15ë¶„
            const randomStart2 = randomStart + 15;
            const h2 = Math.floor(randomStart2 / 60);
            const m2 = randomStart2 % 60;
            const t2 = `0${h2}:${m2 < 10 ? '0'+m2 : m2}`;

            document.getElementById('t1-start').innerText = t1;
            document.getElementById('t1-end').innerText = t1;
            document.getElementById('t2-start').innerText = t2;
            document.getElementById('t2-end').innerText = t2;
        };
    </script>
