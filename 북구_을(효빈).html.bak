            if (query) { location.href = `${query}.html`; } else { alert('검색어를 입력해주세요.'); }
        }
        document.addEventListener('DOMContentLoaded', function() {
            const tooltip = document.getElementById('wiki-tooltip');
            const footnotes = document.querySelectorAll('sup a[id^="rfn-"]');
            footnotes.forEach(fn => {
                fn.addEventListener('mouseenter', function(e) {
                    const targetId = this.getAttribute('href').substring(1); 
                    const content = document.getElementById(targetId);
                    if (content) {
                        let html = content.innerHTML;
                        html = html.replace(/<a[^>]*>\^<\/a>/, ''); 
                        tooltip.innerHTML = html;
                        tooltip.style.display = 'block';
                        const rect = this.getBoundingClientRect();
                        tooltip.style.left = rect.left + 'px';
                        tooltip.style.top = (rect.bottom + window.scrollY + 5) + 'px';
                    }
                });
                fn.addEventListener('mouseleave', function() { tooltip.style.display = 'none'; });
            });
        });
    </script>
