    <script>
        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; 
            box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }

        // ê°ì£¼ íˆ´íŒ ê¸°ëŠ¥ êµ¬í˜„
        document.addEventListener('DOMContentLoaded', () => {
            const tooltip = document.getElementById('wiki-tooltip');
            const footnotes = document.querySelectorAll('.wiki-fn');

            footnotes.forEach(fn => {
                fn.addEventListener('mouseenter', (e) => {
                    const href = fn.getAttribute('href');
                    if (href && href.startsWith('#')) {
                        const targetId = href.substring(1); // '#' ì œê±°
                        const targetEl = document.getElementById(targetId);
                        if (targetEl) {
                            tooltip.innerHTML = targetEl.innerHTML;
                            tooltip.style.display = 'block';
                            
                            // ì´ˆê¸° ìœ„ì¹˜ ì„¤ì • (ë§ˆìš°ìŠ¤ ê·¼ì²˜)
                            tooltip.style.left = (e.pageX + 15) + 'px';
                            tooltip.style.top = (e.pageY + 15) + 'px';
                        }
                    }
                });

                fn.addEventListener('mousemove', (e) => {
                    tooltip.style.left = (e.pageX + 15) + 'px';
                    tooltip.style.top = (e.pageY + 15) + 'px';
                });

                fn.addEventListener('mouseleave', () => {
                    tooltip.style.display = 'none';
                });
            });
        });
    </script>
