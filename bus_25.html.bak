    <script>
        // ëœë¤ ì‹œê°„ ìƒì„± ë¡œì§
        window.onload = function() {
            // ëœë¤ ì²«ì°¨ ì‹œê°„ ìƒì„± (04:45 ~ 05:45)
            const minMins = 4 * 60 + 45; 
            const maxMins = 5 * 60 + 45; 
            
            // 25ë²ˆ ì²«ì°¨
            const r1 = Math.floor(Math.random() * (maxMins - minMins + 1)) + minMins;
            const h1 = Math.floor(r1 / 60);
            const m1 = r1 % 60;
            const t1 = `0${h1}:${m1 < 10 ? '0'+m1 : m1}`;
            
            // 52ë²ˆ ì²«ì°¨ (ë¹„ìŠ·í•œ ì‹œê°„ëŒ€)
            const r2 = Math.floor(Math.random() * (maxMins - minMins + 1)) + minMins;
            const h2 = Math.floor(r2 / 60);
            const m2 = r2 % 60;
            const t2 = `0${h2}:${m2 < 10 ? '0'+m2 : m2}`;

            // í‘œ ìƒë‹¨ ì—…ë°ì´íŠ¸
            document.getElementById('t1-start').innerText = t1;
            document.getElementById('t1-end').innerText = t1;
            document.getElementById('t2-start').innerText = t2;
            document.getElementById('t2-end').innerText = t2;

            // ê²€ìƒ‰ì–´ ì²˜ë¦¬
            const urlParams = new URLSearchParams(window.location.search);
            const query = urlParams.get('q');
            if (query) {
                document.getElementById('searchInput').value = query;
                handleSearch();
            }
        };

        // ê²€ìƒ‰ ê¸°ëŠ¥
        function handleSearch() {
            const searchInput = document.getElementById('searchInput');
            const query = searchInput.value.trim();
            const contextArea = document.getElementById('wiki-context-area');
            const marks = contextArea.querySelectorAll('mark.highlight');
            marks.forEach(mark => {
                const text = document.createTextNode(mark.textContent);
                mark.parentNode.replaceChild(text, mark);
            });
            contextArea.normalize();

            if (!query) { showMsg('ê²€ìƒ‰ì–´ë¥¼ ì…ë ¥í•´ì£¼ì„¸ìš”.'); return; }

            let foundCount = 0;
            let firstMatch = null;

            function highlightText(node) {
                if (node.nodeType === 3) { 
                    const text = node.nodeValue;
                    if (!text.trim()) return;
                    const regex = new RegExp(`(${escapeRegExp(query)})`, 'gi');
                    if (regex.test(text)) {
                        foundCount++;
                        const span = document.createElement('span');
                        span.innerHTML = text.replace(regex, '<mark class="highlight">$1</mark>');
                        const fragment = document.createDocumentFragment();
                        while (span.firstChild) {
                            const child = span.firstChild;
                            if (child.nodeName === 'MARK' && !firstMatch) firstMatch = child;
                            fragment.appendChild(child);
                        }
                        node.parentNode.replaceChild(fragment, node);
                    }
                } else if (node.nodeType === 1 && node.nodeName !== 'SCRIPT' && node.nodeName !== 'STYLE' && node.nodeName !== 'MARK') {
                    Array.from(node.childNodes).forEach(child => highlightText(child));
                }
            }

            highlightText(contextArea);

            if (foundCount > 0) {
                showMsg(`ì´ ${foundCount}ê±´ì˜ ê²€ìƒ‰ ê²°ê³¼ê°€ ìˆìŠµë‹ˆë‹¤.`);
                if (firstMatch) firstMatch.scrollIntoView({ behavior: 'smooth', block: 'center' });
            } else {
                showMsg(`"${query}"ì— ëŒ€í•œ ê²€ìƒ‰ ê²°ê³¼ê°€ ì—†ìŠµë‹ˆë‹¤.`);
            }
        }

        function escapeRegExp(string) { return string.replace(/[.*+?^${}()|[\]\\]/g, '\\$&'); }
        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; 
            box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2500);
        }
        
        // í˜¸ë²„ë³´ë“œ ê¸°ëŠ¥
        document.addEventListener('DOMContentLoaded', () => {
            const refs = document.querySelectorAll('.footnote-ref');
            const hoverboard = document.getElementById('hoverboard');
            let hideTimeout;

            refs.forEach(ref => {
                ref.addEventListener('mouseenter', (e) => {
                    clearTimeout(hideTimeout);
                    const fnId = ref.getAttribute('href').substring(1);
                    const fnContent = document.getElementById(fnId);
                    if (fnContent) {
                        hoverboard.innerHTML = fnContent.innerHTML;
                        const rect = ref.getBoundingClientRect();
                        const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                        const scrollLeft = window.pageXOffset || document.documentElement.scrollLeft;
                        hoverboard.style.top = (rect.bottom + scrollTop + 5) + 'px';
                        if (rect.left + 350 > window.innerWidth) {
                            hoverboard.style.left = 'auto'; hoverboard.style.right = '20px';
                        } else {
                            hoverboard.style.left = (rect.left + scrollLeft) + 'px';
                        }
                        hoverboard.style.display = 'block';
                    }
                });
                ref.addEventListener('mouseleave', () => {
                    hideTimeout = setTimeout(() => { hoverboard.style.display = 'none'; }, 300);
                });
            });
            hoverboard.addEventListener('mouseenter', () => { clearTimeout(hideTimeout); });
            hoverboard.addEventListener('mouseleave', () => { hoverboard.style.display = 'none'; });
        });
    </script>

