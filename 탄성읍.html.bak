    <script>
        // 다크모드 토글
        function toggleTheme() {
            const body = document.body;
            const isDark = body.getAttribute('data-theme') === 'dark';
            body.setAttribute('data-theme', isDark ? 'light' : 'dark');
            localStorage.setItem('wiki_theme', isDark ? 'light' : 'dark');
            document.getElementById('themeBtn').innerText = isDark ? '다크모드' : '라이트모드';
        }

        // [펼치기/접기] 토글 기능 (ID 기반)
        function toggleDetails(id) {
            const el = document.getElementById(id);
            if (el.style.display === 'none') {
                el.style.display = 'block';
            } else {
                el.style.display = 'none';
            }
        }

        // 메시지 박스
        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }

        // 초기 테마 로드
        window.onload = function() {
            const savedTheme = localStorage.getItem('wiki_theme');
            if (savedTheme === 'dark') document.body.setAttribute('data-theme', 'dark');
        };

        // 검색바 엔터키
        document.getElementById('searchInput')?.addEventListener('keypress', function(e) {
            if (e.key === 'Enter' && this.value.trim()) {
                location.href = this.value.trim() + ".html";
            }
        });

        // 각주 호버보드 스크립트
        document.addEventListener('DOMContentLoaded', () => {
            const tooltip = document.getElementById('wiki-tooltip');
            const fnLinks = document.querySelectorAll('.wiki-ref a');

            fnLinks.forEach(link => {
                link.addEventListener('mouseenter', (e) => {
                    const fnId = link.getAttribute('data-id');
                    const fnContent = document.getElementById(fnId);
                    
                    if (fnContent) {
                        tooltip.innerHTML = fnContent.innerHTML;
                        tooltip.style.display = 'block';
                        
                        const rect = link.getBoundingClientRect();
                        const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                        const scrollLeft = window.pageXOffset || document.documentElement.scrollLeft;
                        
                        let top = rect.bottom + scrollTop + 5;
                        let left = rect.left + scrollLeft + 10;

                        if (left + 320 > window.innerWidth) {
                            left = window.innerWidth - 340;
                        }

                        tooltip.style.top = top + 'px';
                        tooltip.style.left = left + 'px';
                    }
                });

                link.addEventListener('mouseleave', () => {
                    tooltip.style.display = 'none';
                });
            });
        });
    </script>
