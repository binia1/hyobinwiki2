    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@300;400;700&display=swap');
        
        :root { 
            /* [설정] 탄성군 퍼스널 컬러 (#BBFF64) */
            --wiki-main: #BBFF64; 
            --wiki-main-text: #000000; /* 밝은 배경 위 검은 글씨 */
            
            --wiki-fixed: #7777AA; 
            --wiki-point: #7CB342; /* 강조색 */
            
            --wiki-border: #ccc; 
            --wiki-bg: #ffffff; 
            --wiki-text: #373a3c; 
            --wiki-gray-bg: #f9f9f9; 
            --wiki-link: #0062cc; 
            --wiki-table-header: var(--wiki-main);
            --wiki-table-header-text: var(--wiki-main-text);
            --wiki-tooltip-bg: #ffffff;
            --wiki-tooltip-text: #333333;
            --wiki-tooltip-border: #aaaaaa;
        }

        [data-theme='dark'] { 
            --wiki-fixed: #9999CC;
            --wiki-main: #9E9D24; 
            --wiki-main-text: #ffffff;
            --wiki-point: #C0CA33;

            --wiki-border: #444; 
            --wiki-bg: #121212; 
            --wiki-text: #eeeeee; 
            --wiki-gray-bg: #2d2f34; 
            --wiki-link: #aaaadd; 
            --wiki-table-header-text: #ffffff;
            --wiki-tooltip-bg: #2d2f34;
            --wiki-tooltip-text: #eeeeee;
            --wiki-tooltip-border: #555555;
        }

        body { 
            font-family: 'Noto Sans KR', sans-serif; 
            background-color: var(--wiki-bg); 
            color: var(--wiki-text); 
            line-height: 1.6; 
            margin: 0; 
            transition: background-color 0.3s, color 0.3s;
        }

        .wiki-container { 
            max-width: 1280px; 
            margin: 0 auto; 
            padding: 20px 40px; 
            border-left: 1px solid var(--wiki-border); 
            border-right: 1px solid var(--wiki-border); 
            min-height: 100vh; 
            background-color: var(--wiki-bg);
            display: flex;
            flex-direction: column;
        }

        /* 테이블 공통 */
        table { border-collapse: collapse !important; width: 100% !important; margin: 15px 0; border: 1px solid var(--wiki-border) !important; table-layout: fixed !important; }
        th, td { border: 1px solid var(--wiki-border) !important; padding: 6px 8px; vertical-align: middle; font-size: 0.85rem; }
        th { background-color: var(--wiki-gray-bg); font-weight: bold; text-align: center; }
        
        .wiki-table-head { background-color: var(--wiki-main) !important; color: var(--wiki-main-text) !important; font-weight: bold; text-align: center; }

        .wiki-link { color: var(--wiki-link); font-weight: bold; cursor: pointer; text-decoration: none; display: inline-block; }
        .wiki-link:hover { text-decoration: underline; }
        
        h1 { font-size: 2.5rem !important; font-weight: 700 !important; margin-bottom: 1.5rem; line-height: 1.2; border-bottom: 2px solid var(--wiki-main); padding-bottom: 10px; }
        h2 { font-size: 1.8rem !important; font-weight: 700 !important; margin-top: 2.5rem; margin-bottom: 1rem; border-bottom: 1px solid var(--wiki-border); padding-bottom: 0.3rem; display: flex; align-items: center; gap: 10px; }
        h2::before { content: ""; display: inline-block; width: 8px; height: 1.8rem; background: var(--wiki-main); }
        h3 { font-size: 1.4rem !important; font-weight: 700 !important; margin-top: 1.8rem; margin-bottom: 0.8rem; color: var(--wiki-text); border-left: 5px solid var(--wiki-point); padding-left: 12px; }
        
        blockquote.wiki-quote {
            border-left: 5px solid var(--wiki-point);
            background-color: var(--wiki-gray-bg);
            padding: 10px 15px;
            margin: 10px 0;
            font-style: italic;
            color: #555;
        }
        [data-theme='dark'] blockquote.wiki-quote { color: #aaa; }
        blockquote.wiki-quote strong { color: var(--wiki-text); font-weight: bold; }

        .infobox { 
            border: 2px solid var(--wiki-main); 
            background-color: #fff; 
            width: 100%; 
            max-width: 400px; 
            font-size: 0.85rem; 
            margin-bottom: 20px; 
            float: right; 
            margin-left: 25px; 
            border-radius: 4px; 
            overflow: hidden; 
            box-shadow: 0 2px 5px rgba(0,0,0,0.05);
        }
        [data-theme='dark'] .infobox { background-color: #2d2f34; border-color: var(--wiki-main); }
        .infobox-header { background-color: var(--wiki-main); color: var(--wiki-main-text); padding: 8px; text-align: center; font-weight: bold; font-size: 1.1em; }
        .infobox-subheader { background-color: var(--wiki-main); color: var(--wiki-main-text); padding: 4px; text-align: center; font-weight: bold; font-size: 0.9em; opacity: 0.9; }
        .infobox th { background: #f5f5f5; width: 110px; text-align: center; padding: 5px 8px; border-bottom: 1px solid #ddd; color: #333; font-weight: bold; }
        [data-theme='dark'] .infobox th { background: #383b40; color: #eee; border-color: #555; }
        .infobox td { padding: 6px 10px; text-align: left; border-bottom: 1px solid #ddd; }
        [data-theme='dark'] .infobox td { border-color: #555; }

        details.wiki-details { border: 1px solid var(--wiki-border); border-radius: 4px; margin: 5px 0; background: var(--wiki-bg); }
        details.wiki-details summary { cursor: pointer; padding: 6px 10px; background-color: var(--wiki-gray-bg); font-size: 0.85rem; text-align: center; list-style: none; font-weight: bold; color: var(--wiki-link); }
        details.wiki-details[open] summary { border-bottom: 1px solid var(--wiki-border); }
        details.wiki-details .content { padding: 10px; font-size: 0.9rem; }

        .party-blue { background-color: #004EA2; color: white; padding: 2px 6px; border-radius: 4px; font-size: 0.8em; }
        .del { text-decoration: line-through; color: gray; }
        .disambiguation { font-size: 0.9em; color: #555; background: #f9f9f9; padding: 5px 10px; border-bottom: 1px solid #eee; margin-bottom: 15px; }
        .disambiguation i { color: #888; margin-right: 5px; }

        .wiki-ref { font-size: 0.75em; vertical-align: super; line-height: 0; margin-left: 2px; font-weight: normal; }
        .wiki-ref a { color: var(--wiki-link); text-decoration: none; cursor: pointer; }
        .wiki-footnotes { margin-top: 40px; padding-top: 20px; border-top: 1px solid var(--wiki-border); font-size: 0.9rem; }
        #wiki-tooltip { position: absolute; display: none; background-color: var(--wiki-tooltip-bg); color: var(--wiki-tooltip-text); border: 1px solid var(--wiki-tooltip-border); padding: 10px 15px; border-radius: 6px; box-shadow: 0 4px 15px rgba(0,0,0,0.2); font-size: 0.85rem; z-index: 9999; max-width: 320px; pointer-events: none; line-height: 1.4; }

        .nav-table-container { border: 2px solid var(--wiki-main); border-radius: 6px; overflow: hidden; margin-bottom: 20px; }
        .nav-table-header { background: var(--wiki-main); color: var(--wiki-main-text); text-align: center; font-weight: bold; padding: 6px; cursor: pointer; }
        
        /* [중요] 내비게이션 내부 테이블 스타일 (4열 강제) */
        .inner-nav-table td { width: 25% !important; border: 1px solid #ddd !important; }
        [data-theme='dark'] .inner-nav-table td { border-color: #555 !important; }

        .toc { display: inline-block; min-width: 300px; border: 1px solid var(--wiki-border); background: var(--wiki-gray-bg); padding: 15px; border-radius: 4px; margin-bottom: 20px; }
        .toc-title { text-align: center; font-weight: bold; border-bottom: 1px solid var(--wiki-border); margin-bottom: 10px; padding-bottom: 5px; }
        .toc ul { list-style: none; padding: 0; margin: 0; font-size: 0.9rem; }
        .toc ul ul { padding-left: 20px; }

        .wiki-tool-btn { padding: 4px 12px; font-size: 11px; border-right: 1px solid var(--wiki-border); cursor: pointer; color: inherit; display: flex; align-items: center; text-decoration: none; }
        .wiki-tool-btn.active { background-color: var(--wiki-main); color: var(--wiki-main-text) !important; font-weight: bold; }
        .wiki-btn-group { display: flex; background: #fff; border: 1px solid var(--wiki-border); border-radius: 4px; overflow: hidden; }
        [data-theme='dark'] .wiki-btn-group { background: #2d2f34; }
    </style>
</head>
