            if(btn) btn.innerText = 'ë¼ì´íŠ¸ëª¨ë“œ';
        }
    };

    document.getElementById('searchInput')?.addEventListener('keypress', function(e) {
        if (e.key === 'Enter') handleSearchFromNav(this.value.trim());
    });
</script>
<script>
    // -----------------------------------------------------------
    // [í†µí•© í•´ê²°] 1. ID ìë™ ë¶€ì—¬ + 2. ë¶€ë“œëŸ¬ìš´ ì–‘ë°©í–¥ ì´ë™
    // -----------------------------------------------------------
    document.addEventListener('DOMContentLoaded', function() {
        
        // 1. [ìë™ ìˆ˜ë¦¬] ë³¸ë¬¸ ë§í¬ì— ëŒì•„ì˜¬ ì£¼ì†Œ(ID)ê°€ ì—†ìœ¼ë©´ ìë™ìœ¼ë¡œ ë¶™ì—¬ì¤ë‹ˆë‹¤.
        // ì˜ˆ: href="#fn-1" ì¸ ë…€ì„ì—ê²Œ ìë™ìœ¼ë¡œ id="fnref-1" ì„ ë‹¬ì•„ì¤Œ
        const triggers = document.querySelectorAll('.wiki-fn-trigger');
        triggers.forEach(function(trigger) {
            const href = trigger.getAttribute('href'); // "#fn-1"
            if (href && href.startsWith('#fn-')) {
                const num = href.replace('#fn-', ''); // "1"
                // IDê°€ ì—†ìœ¼ë©´ ë‹¬ì•„ì¤Œ
                if (!trigger.id) {
                    trigger.id = 'fnref-' + num;
                }
            }
        });

        // 2. [ì–‘ë°©í–¥ ì´ë™] ìœ„ë¡œ ê°€ë“  ì•„ë˜ë¡œ ê°€ë“  ë¶€ë“œëŸ½ê²Œ ì´ë™
        document.addEventListener('click', function(e) {
            // í´ë¦­ëœ ìš”ì†Œê°€ ë§í¬ì¸ì§€ í™•ì¸ (ë³¸ë¬¸ ê°ì£¼ or ì•„ë˜ìª½ ë˜ëŒì•„ê°€ê¸° ë²„íŠ¼)
            const link = e.target.closest('a');
            
            // ë§í¬ê°€ ì•„ë‹ˆê±°ë‚˜ ë‚´ë¶€ ì´ë™(#)ì´ ì•„ë‹ˆë©´ ë¬´ì‹œ
            if (!link || !link.getAttribute('href') || !link.getAttribute('href').startsWith('#')) return;

            const href = link.getAttribute('href');
            // ì˜ˆì™¸: ëª©ì°¨ í† ê¸€ ë“± ë‹¤ë¥¸ ê¸°ëŠ¥ì€ ê±´ë“œë¦¬ì§€ ì•ŠìŒ
            if (href === '#' || href.includes('toc')) return;

            const targetId = href.substring(1); // # ì œê±°
            const targetElem = document.getElementById(targetId);

            if (targetElem) {
                e.preventDefault(); // íŒ! íŠ€ëŠ”ê±° ë°©ì§€

                // ë¶€ë“œëŸ½ê²Œ ìŠ¤í¬ë¡¤
                targetElem.scrollIntoView({
                    behavior: 'smooth',
                    block: 'center' // í™”ë©´ ì¤‘ì•™ì— ì˜¤ë„ë¡
                });

                // ë„ì°©í–ˆë‹¤ëŠ” í‘œì‹œ (ë…¸ë€ìƒ‰ ê¹œë¹¡ì„)
                targetElem.style.transition = 'background-color 0.5s ease';
                const originalBg = targetElem.style.backgroundColor;
                targetElem.style.backgroundColor = 'rgba(255, 235, 59, 0.5)'; // ë…¸ë€ í˜•ê´‘íœ ìƒ‰
                
                setTimeout(() => {
                    targetElem.style.backgroundColor = originalBg || 'transparent';
                }, 1500);
            }
        });
    });
</script>
