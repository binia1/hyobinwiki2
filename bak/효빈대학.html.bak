    <script>
        // 검색 기능 핸들러
        function handleSearch() {
            const query = document.getElementById('searchInput').value;
            if (query) {
                // 나무위키 검색으로 리다이렉트 (실제 구현 시 내부 검색으로 변경)
                window.open(`https://namu.wiki/w/${encodeURIComponent(query)}`, '_blank');
            } else {
                showMsg('검색어를 입력해주세요.');
            }
        }

        // 메시지 박스 표시 함수
        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; 
            box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }

        // 링크 이동 핸들러 (파일 존재 여부 체크 시뮬레이션)
        async function goToLink(filename) {
            // 1. 일반적인 나무위키 키워드인 경우 (예외 처리)
            const namuKeywords = ['대한민국', '국립대학', '호랑이', '전화기', '국가거점국립대학교총장협의회', '서울대학교'];
            const pureName = filename.replace('.html', '').replace(/_/g, ' ');
            
            if (namuKeywords.includes(pureName) || filename.includes('namu.wiki')) {
                window.open(`https://namu.wiki/w/${encodeURIComponent(pureName)}`, '_blank');
                return;
            }

            // 2. 로컬 파일 체크 (fetch 사용)
            try {
                // 실제 파일이 있는지 HEAD 요청을 보내봄 (로컬 서버 환경 필요, 단순 파일 열기로는 제한될 수 있음)
                // 단순 파일 열기(file://) 환경에서는 fetch가 작동하지 않을 수 있으므로, 
                // 에러 발생 시 그냥 이동 시도하거나 메시지를 띄우는 방식으로 처리해야 함.
                
                location.href = filename;
                
            } catch (e) {
                showMsg('문서를 찾을 수 없습니다.');
            }
        }
        
        // 목차 스무스 스크롤
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const targetId = this.getAttribute('href');
                const targetElement = document.querySelector(targetId);
                
                if(targetElement){
                    window.scrollTo({
                        top: targetElement.offsetTop - 80, 
                        behavior: 'smooth'
                    });
                }
            });
        });
    </script>
