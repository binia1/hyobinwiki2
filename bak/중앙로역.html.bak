        .wiki-tool-btn:hover { background: #f0f0f0; }

        .wiki-footer { margin-top: 100px; padding: 50px 20px; background-color: var(--wiki-footer-bg); border-top: 1px solid var(--wiki-border); color: var(--wiki-footer-text); font-size: 0.8rem; line-height: 1.8; }
        .wiki-footer-container { max-width: 1200px; margin: 0 auto; }
        .wiki-footer-disclaimer { margin-top: 15px; padding-top: 15px; border-top: 1px solid #ddd; }

        .wiki-link { color: var(--wiki-link); font-weight: 900; cursor: pointer; text-decoration: none; }
        .wiki-link:hover { text-decoration: underline; }
        .category-box { background: #f9f9f9; border: 1px solid var(--wiki-border); padding: 10px 16px; font-size: 0.85rem; margin: 15px 0; border-radius: 4px; }
        .toc { display: inline-block; min-width: 320px; border: 1px solid var(--wiki-border); background: var(--wiki-gray-bg); padding: 20px; border-radius: 4px; margin-bottom: 25px; }
