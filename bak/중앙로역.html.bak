        .infobox-station-name .other-name { font-size: 0.85rem; color: #666; font-weight: normal; }
        
        .infobox-rail-nav { display: flex; width: 100%; border-bottom: 1px solid var(--wiki-border); align-items: stretch; min-height: 90px; background: #fff; }
        .rail-nav-prev, .rail-nav-next { flex: 1; display: flex; flex-direction: column; justify-content: center; align-items: center; font-size: 0.82rem; color: #888 !important; line-height: 1.5; padding: 15px 10px; text-decoration: none !important; transition: background 0.2s; }
        .rail-nav-prev:hover, .rail-nav-next:hover { background: #f8fafc; }
        .rail-nav-prev b, .rail-nav-next b { font-size: 1.1rem; color: #000 !important; margin: 3px 0; display: block; font-weight: 900; }
        
        .rail-nav-center { flex: none; width: 110px; display: flex; flex-direction: column; justify-content: center; align-items: center; color: white; font-weight: 900; font-size: 1rem; border-left: 1px solid var(--wiki-border); border-right: 1px solid var(--wiki-border); text-shadow: 0 1px 2px rgba(0,0,0,0.2); }
        .nav-line-name { font-size: 0.7rem; font-weight: 900; opacity: 0.9; margin-bottom: 2px; background: rgba(0,0,0,0.1); padding: 1px 6px; border-radius: 10px; }

