        .line-badge-txt { display: inline-flex; align-items: center; justify-content: center; width: 1.5rem; height: 1.5rem; border-radius: 50%; background-color: white; font-size: 0.8rem; font-weight: 900; border: 2px solid; line-height: 1; margin-right: 5px; }
        .rect-badge-txt { display: inline-flex; align-items: center; justify-content: center; width: 1.5rem; height: 1.5rem; border-radius: 4px; background-color: white; font-size: 0.8rem; font-weight: 900; border: 2px solid; line-height: 1; margin-right: 5px; }
        
        #wiki-msg-box { position: fixed; bottom: 30px; left: 50%; transform: translateX(-50%); background: rgba(0,0,0,0.8); color: white; padding: 12px 28px; border-radius: 50px; font-size: 14px; z-index: 10000; display: none; }
    
        /* [플랫폼/지도 이미지 고정 vFinal] */
        .platform-media-box { overflow:hidden; }
        .platform-media-box img { width:100%; height:100%; object-fit:cover; display:block; }
        .infobox-map { overflow:hidden; }
        .infobox-map img { width:100%; height:100%; object-fit:cover; display:block; }
        .infobox-image { overflow:hidden; }
        .infobox-image img { width:100%; height:100%; object-fit:cover; display:block; }
</style>
