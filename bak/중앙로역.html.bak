        .track-normal { color: #000 !important; font-family: monospace; font-size: 1.25rem; }
        .track-express { color: #EE0022 !important; font-family: monospace; font-size: 1.25rem; font-weight: 900; }
        .platform-badge-box { color: white; padding: 4px 10px; border-radius: 3px; font-size: 0.85rem; font-weight: 900; }

        .nav-box { border: 2px solid var(--wiki-main); margin-top: 50px; font-size: 0.9rem; border-radius: 4px; overflow: hidden; }
        .nav-box-header { background-color: var(--wiki-main); color: white; padding: 10px; font-weight: 900; text-align: center; }
