        .infobox-main-row { display: flex; border-bottom: 1px solid var(--wiki-border); background-color: #ffffff; color: #000000; }
        .infobox-line-badge { width: 95px; display: flex; align-items: center; justify-content: center; font-weight: 900; font-size: 1rem; flex-shrink: 0; border-right: 1px solid rgba(0,0,0,0.1); background-color: #f8f9fa; color: #000; text-align: center; }
        .infobox-station-name { flex: 1; padding: 15px; text-align: center; display: flex; flex-direction: column; gap: 4px; }
        .infobox-station-name b { font-size: 1.8rem; color: #000; line-height: 1; font-weight: 900; }
