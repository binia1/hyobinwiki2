        }

        body { font-family: 'Noto Sans KR', sans-serif; background-color: var(--wiki-bg); color: var(--wiki-text); line-height: 1.7; word-break: keep-all; margin: 0; }
        .wiki-container { max-width: 1200px; margin: 0 auto; padding: 20px 40px; border-left: 1px solid var(--wiki-border); border-right: 1px solid var(--wiki-border); min-height: 100vh; background-color: var(--wiki-bg); }

        .txt-lvl-1 { font-size: 3.5rem; font-weight: 800; color: #000000; margin-bottom: 1rem; line-height: 1.1; letter-spacing: -1.5px; } 
        
        h2 { 
