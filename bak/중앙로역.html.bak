            --wiki-line-1: #0077DD;
            --wiki-line-2: #00CCAA;
            --wiki-line-7: #FF8899;
            
            --wiki-line-color: var(--wiki-line-1); /* 대표 색상은 1호선 기준 */
            --wiki-header-bg: #ffffff;
            --wiki-header-text: #000000;
            --wiki-table-header-bg: #ffffff;
