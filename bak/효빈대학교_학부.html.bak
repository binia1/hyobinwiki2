        .wiki-container { max-width: 1300px; margin: 0 auto; padding: 20px 40px; border-left: 1px solid var(--wiki-border); border-right: 1px solid var(--wiki-border); min-height: 100vh; background-color: var(--wiki-bg); }

        .txt-lvl-1 { font-size: 3.5rem; font-weight: 900 !important; color: #000000; line-height: 1.1; letter-spacing: -1.5px; margin-bottom: 0; } 
        .title-area { display: flex; justify-content: space-between; align-items: flex-end; border-bottom: 2px solid #ccc; margin-bottom: 20px; padding-bottom: 10px; }

        h2 { font-size: 1.8rem; font-weight: 700; border-bottom: 1px solid var(--wiki-border); padding-bottom: 5px; margin-top: 2.5rem; display: flex; align-items: center; gap: 10px; color: var(--wiki-main); clear: both; }
        h2::before { content: ""; display: inline-block; width: 5px; height: 1.8rem; background: var(--wiki-main); }
        h3 { font-size: 1.4rem; font-weight: 700; margin-top: 1.8rem; border-left: 4px solid var(--wiki-main); padding-left: 12px; color: var(--wiki-main); }
        h4 { font-size: 1.1rem; font-weight: 700; margin-top: 1.2rem; margin-bottom: 0.5rem; color: #444; border-left: 2px solid #ccc; padding-left: 8px; }

        .wiki-link { color: var(--wiki-link); font-weight: bold; cursor: pointer; text-decoration: none; }
        .wiki-link:hover { text-decoration: underline; }
        
        .dept-box { background-color: #fff; border: 1px solid #ddd; padding: 15px; margin-top: 10px; border-radius: 4px; box-shadow: 0 1px 2px rgba(0,0,0,0.05); }
        .dept-list { display: flex; flex-wrap: wrap; gap: 8px; font-size: 0.95rem; }
        .dept-item { display: inline-block; background-color: #f4f4f9; padding: 4px 8px; border-radius: 4px; border: 1px solid #e0e0e0; transition: all 0.2s; cursor: pointer; color: #373a3c; }
        .dept-item:hover { background-color: var(--wiki-main); color: #fff; border-color: var(--wiki-main); }

        .meme-box { background-color: #f0f8ff; border: 1px solid #b0c4de; padding: 15px; margin: 15px 0; border-radius: 4px; }
        .meme-title { font-weight: bold; color: #0055AA; margin-bottom: 5px; display: block; }

        /* Navigation Box Style */
        .nav-table { width: 100%; border-collapse: collapse; margin-top: 60px; border: 2px solid var(--wiki-main); }
        .nav-header { background-color: var(--wiki-main); color: white; text-align: center; font-weight: bold; padding: 8px; font-size: 1.1rem; }
        .nav-subheader { background-color: #f0f0f0; color: #333; text-align: center; font-weight: bold; padding: 6px; border-bottom: 1px solid #ccc; font-size: 0.95rem; }
        .nav-content { background-color: #fff; padding: 10px; text-align: center; border-bottom: 1px solid #eee; }
        .nav-content a { margin: 0 10px; display: inline-block; font-size: 0.9rem; color: #373a3c; text-decoration: none; font-weight: bold; }
        .nav-content a:hover { text-decoration: underline; color: var(--wiki-link); }
        
        #wiki-msg-box { position: fixed; bottom: 30px; left: 50%; transform: translateX(-50%); background: rgba(0,0,0,0.8); color: white; padding: 10px 24px; border-radius: 50px; font-size: 14px; z-index: 10000; display: none; }
    </style>
</head>
