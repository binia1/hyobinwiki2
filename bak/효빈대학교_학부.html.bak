    <script>
        function handleSearch() {
            const query = document.getElementById('searchInput').value;
            if (query) {
                w(query);
            } else {
                showMsg('검색어를 입력해주세요.');
            }
        }

        const namuDocs = ["대한민국", "거점국립대학교", "서울대학교", "사회복지학", "철도", "고양이", "오타쿠/성지", "전북대학교", "둘러보기 틀/단과대학", "둘러보기 틀/학과", "러브 라이브! 니지가사키", "봇치 더 락!", "걸즈 밴드 크라이", "뱅드림", "HAF"];

        function w(title) {
            if (namuDocs.includes(title)) {
                window.open(`https://namu.wiki/w/${encodeURIComponent(title)}`, '_blank');
            } else {
                location.href = `${title}.html`;
            }
        }

        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }
    </script>
