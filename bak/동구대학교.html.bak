    <script>
        // 검색 기능 핸들러
        function handleSearch() {
            const query = document.getElementById('searchInput').value;
            if (query) {
                window.open(`https://namu.wiki/w/${encodeURIComponent(query)}`, '_blank');
            } else {
                showMsg('검색어를 입력해주세요.');
            }
        }

        // 메시지 박스 표시 함수
        function showMsg(text) {
            const box = document.getElementById('wiki-msg-box');
            box.innerText = text; 
            box.style.display = 'block';
            setTimeout(() => { box.style.display = 'none'; }, 2000);
        }

        // 링크 이동 핸들러
        async function goToLink(filename) {
            if (filename.startsWith('#')) return;

            const namuKeywords = ['대한민국', '국립대학', '사립대학'];
            const pureName = filename.replace('.html', '').replace(/_/g, ' ');
            
            if (namuKeywords.includes(pureName) || filename.includes('namu.wiki')) {
                window.open(`https://namu.wiki/w/${encodeURIComponent(pureName)}`, '_blank');
                return;
            }

            try {
                location.href = filename;
            } catch (e) {
                showMsg('문서를 찾을 수 없습니다.');
            }
        }
        
        // 목차 및 내부 링크 스무스 스크롤
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                const targetId = this.getAttribute('href');
                if (!targetId || targetId === '#') return;

                e.preventDefault();
                const targetElement = document.querySelector(targetId);
                
                if(targetElement){
                    window.scrollTo({
                        top: targetElement.offsetTop - 80, 
                        behavior: 'smooth'
                    });
                    
                    targetElement.style.transition = 'background 0.5s';
                    targetElement.style.backgroundColor = '#fff3cd';
                    setTimeout(() => { targetElement.style.backgroundColor = 'transparent'; }, 1500);
                }
            });
        });

        // --- 각주 툴팁 호버보드 로직 ---
        const tooltip = document.getElementById('wiki-fn-tooltip');
        const footnotes = document.querySelectorAll('.wiki-fn');

        footnotes.forEach(fn => {
            fn.addEventListener('mouseenter', (e) => {
                const targetId = fn.getAttribute('href').substring(1); 
                const contentEl = document.getElementById(targetId);
                
                if (contentEl) {
                    tooltip.innerHTML = contentEl.innerHTML;
                    const backLink = tooltip.querySelector('.wiki-fn-back');
                    if(backLink) {
                        backLink.style.display = 'none'; 
                    }
                    tooltip.style.display = 'block';
                }
            });

            fn.addEventListener('mousemove', (e) => {
                const x = e.clientX + 15;
                const y = e.clientY + 15;
                
                const tooltipRect = tooltip.getBoundingClientRect();
                let finalX = x;
                let finalY = y;

                if (x + tooltipRect.width > window.innerWidth) {
                    finalX = e.clientX - tooltipRect.width - 10;
                }
                if (y + tooltipRect.height > window.innerHeight) {
                    finalY = e.clientY - tooltipRect.height - 10;
                }

                tooltip.style.left = finalX + 'px';
                tooltip.style.top = finalY + 'px';
            });

            fn.addEventListener('mouseleave', () => {
                tooltip.style.display = 'none';
            });
        });
    </script>
