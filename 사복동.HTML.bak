        // 6. ê°ì£¼ íˆ´íŒ ê¸°ëŠ¥
        function initFootnoteTooltips() {
            const footnotes = document.querySelectorAll('.wiki-fn');
            const tooltip = document.getElementById('wiki-tooltip');

            footnotes.forEach(fn => {
                fn.addEventListener('mouseenter', function(e) {
                    const targetId = this.getAttribute('data-target');
                    const contentEl = document.getElementById(targetId);
                    if (contentEl) {
                        let text = contentEl.innerHTML;
                        const tempDiv = document.createElement('div');
                        tempDiv.innerHTML = text;
                        const backLink = tempDiv.querySelector('a:last-child'); 
                        if(backLink && backLink.innerText.includes('â†‘')) backLink.remove();
                        tooltip.innerHTML = tempDiv.innerHTML;
                        tooltip.style.display = 'block';
                        const rect = this.getBoundingClientRect();
                        const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                        tooltip.style.top = (rect.bottom + scrollTop + 5) + 'px';
                        tooltip.style.left = rect.left + 'px';
                    }
                });
                fn.addEventListener('mouseleave', function() {
                    tooltip.style.display = 'none';
                });
                fn.addEventListener('click', function(e) {
                    e.preventDefault();
                    const targetId = this.getAttribute('data-target');
                    const targetEl = document.getElementById(targetId);
                    if(targetEl) {
                        targetEl.scrollIntoView({behavior: 'smooth', block: 'center'});
                        targetEl.style.backgroundColor = "rgba(255, 255, 0, 0.3)";
                        setTimeout(() => targetEl.style.backgroundColor = "transparent", 1500);
                    }
                });
            });
        }
