    <script>
        let isNavOpen = { natNav: true, localNav: true };
        window.onload = function() {
            const savedTheme = localStorage.getItem('wiki_theme');
            if (savedTheme === 'dark') toggleTheme();
        };
        function toggleTheme() {
            const body = document.body;
            const isDark = body.getAttribute('data-theme') === 'dark';
            body.setAttribute('data-theme', isDark ? 'light' : 'dark');
            localStorage.setItem('wiki_theme', isDark ? 'light' : 'dark');
        }
        function toggleNav(id, labelId) {
            const content = document.getElementById(id);
            const label = document.getElementById(labelId);
            if (isNavOpen[id]) { content.style.maxHeight = '0px'; label.innerText = '[펼치기]'; } 
            else { content.style.maxHeight = '2000px'; label.innerText = '[접기]'; }
            isNavOpen[id] = !isNavOpen[id];
        }
        function toggleMoreMenu(e) { e.stopPropagation(); const d = document.getElementById('moreDropdown'); d.style.display = d.style.display === 'flex' ? 'none' : 'flex'; }
        window.onclick = function() { document.getElementById('moreDropdown').style.display = 'none'; };
        function openModal(type) {
            const overlay = document.getElementById('modalOverlay');
            const title = document.getElementById('modalTitle');
            const body = document.getElementById('modalBody');
            overlay.style.display = 'flex';
            if(type === 'backlinks') { title.innerText = "역링크: 효빈광역시"; body.innerHTML = "<li>효빈위키:대문</li><li>효빈</li>"; } 
            else { title.innerText = "ACL: 효빈광역시"; body.innerHTML = "<p>편집: 관리자(효빈) 보호 중</p>"; }
        }
        function closeModal() { document.getElementById('modalOverlay').style.display = 'none'; }
        function toggleFavorite() { document.getElementById('favIcon').classList.toggle('text-yellow-400'); showToast("즐겨찾기 상태 변경됨"); }
        function showToast(msg) {
            const t = document.createElement('div');
            t.className = "fixed bottom-10 left-1/2 -translate-x-1/2 bg-[#7777AA] text-white px-6 py-2 rounded-full text-xs shadow-lg z-[100] transition-opacity";
            t.innerText = msg; document.body.appendChild(t);
            setTimeout(() => { if(t.parentNode) t.remove(); }, 2000);
        }
        function handleSearchFromNav(title) { showToast(`'${title}' 문서로 이동 시뮬레이션 중`); }
// 효빈위키 전용 각주 호버보드 로직
document.addEventListener('mouseover', function(e) {
    const trigger = e.target.closest('.wiki-fn-trigger');
    if (trigger) {
        const tooltip = document.getElementById('wiki-footnote-tooltip');
        const fnId = trigger.getAttribute('href').substring(1); 
        const fnElem = document.getElementById(fnId);
        
        if (fnElem) {
            // 각주 리스트에서 실제 설명이 적힌 span을 가져옴
            const textContent = fnElem.querySelector('span:last-child').innerText;
            tooltip.innerText = textContent;
            tooltip.style.display = 'block';
            
            // 위치 계산
            const rect = trigger.getBoundingClientRect();
            tooltip.style.left = (window.scrollX + rect.left) + 'px';
            tooltip.style.top = (window.scrollY + rect.bottom + 10) + 'px';
        }
    }
});

// 마우스가 떠나면 숨김
document.addEventListener('mouseout', function(e) {
    if (e.target.closest('.wiki-fn-trigger')) {
        document.getElementById('wiki-footnote-tooltip').style.display = 'none';
    }
});    
</script>
