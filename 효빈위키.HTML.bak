        .infobox-name { flex: 1; display: flex; flex-direction: column; justify-content: center; padding: 0 8px; text-align: center; }
        .infobox table { width: 100%; border-collapse: collapse; }
        .infobox th { background: var(--wiki-main); color: white; width: 105px; padding: 5px 8px; border: 1px solid var(--wiki-border); text-align: left; }
        .infobox td { padding: 5px 8px; border: 1px solid var(--wiki-border); }
        
        /* 목차 */
        .toc { border: 1px solid var(--wiki-border); background-color: var(--wiki-gray-bg); padding: 15px; display: inline-block; min-width: 250px; margin-bottom: 20px; }
        .toc-title { font-weight: bold; text-align: center; border-bottom: 1px solid var(--wiki-border); margin-bottom: 10px; padding-bottom: 5px; }
        .toc ul { list-style: none; padding: 0; margin: 0; }
        .toc li { margin-bottom: 4px; font-size: 0.85rem; }

        .wiki-link { color: var(--wiki-link); font-weight: bold; text-decoration: none; cursor: pointer; }
        .wiki-link:hover { text-decoration: underline; }

        /* 내비게이션 박스 */
        .nav-box { border: 1px solid var(--nav-color); margin-bottom: 10px; font-size: 0.8rem; clear: both; overflow: hidden; border-radius: 4px; }
        .nav-box-header { background-color: var(--nav-color); color: white; padding: 8px 12px; font-weight: bold; display: flex; justify-content: center; align-items: center; cursor: pointer; position: relative; }
        .nav-toggle-btn { position: absolute; right: 12px; font-size: 10px; background: rgba(255,255,255,0.2); px-2; py-0.5; rounded; }
        .nav-table { width: 100%; border-collapse: collapse; text-align: center; }
        .nav-table th { background-color: var(--wiki-gray-bg); border: 1px solid var(--wiki-border); padding: 8px; width: 110px; text-align: center; font-weight: bold; }
        .nav-table td { border: 1px solid var(--wiki-border); padding: 8px; text-align: center; }
        
