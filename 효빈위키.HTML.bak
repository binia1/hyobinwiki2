        .infobox { border: 1px solid var(--wiki-main); background-color: var(--wiki-gray-bg); width: 100%; max-width: 360px; font-size: 0.8rem; margin-bottom: 20px; float: right; margin-left: 20px; border-radius: 4px; overflow: hidden; }
