        .wiki-tool-btn { padding: 4px 12px; font-size: 11px; border-right: 1px solid var(--wiki-border); cursor: pointer; color: inherit; text-decoration: none; transition: background 0.2s; }
        .wiki-tool-btn:last-child { border-right: none; }
        .wiki-tool-btn:hover { background-color: rgba(0,0,0,0.05); }
        .wiki-tool-btn.active { background-color: var(--wiki-main); color: white !important; font-weight: bold; }

