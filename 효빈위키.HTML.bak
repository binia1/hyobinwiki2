            border: 1px solid var(--wiki-border);
            margin-bottom: 20px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 4px;
            color: #aaa;
            font-size: 12px;
            position: relative;
        }
        .wiki-ad-area::before {
            content: "AD / 광고";
            position: absolute;
            top: 5px;
            left: 10px;
            font-weight: bold;
            opacity: 0.5;
