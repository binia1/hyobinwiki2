            --bus-express: #D81C2F; /* 급행 - 스칼렛 레드 */
            --bus-wide: #485EC6;    /* 광역 - 로얄 블루 */
            --bus-seat: #FF5800;    /* 좌석 - 오렌지 */
            --bus-main: #01B7ED;    /* 간선 - 스카이 블루 */
            --bus-branch: #37B484;  /* 지선 - 제이드 그린 */
            --bus-circular: #E7D600;/* 순환 - 파스텔 옐로우 */
            --bus-village: #A664A0; /* 마을 - 바이올렛 */
        }

        body { font-family: 'Noto Sans KR', sans-serif; background-color: var(--wiki-bg); color: var(--wiki-text); line-height: 1.6; word-break: keep-all; margin: 0; }
        .wiki-container { max-width: 1300px; margin: 0 auto; padding: 20px 40px; border-left: 1px solid var(--wiki-border); border-right: 1px solid var(--wiki-border); min-height: 100vh; background-color: var(--wiki-bg); position: relative; }

        /* 타이틀 스타일 */
        .txt-lvl-1 { font-size: 3.0rem; font-weight: 900 !important; color: #000000; line-height: 1.1; letter-spacing: -1.5px; margin-bottom: 0; } 
